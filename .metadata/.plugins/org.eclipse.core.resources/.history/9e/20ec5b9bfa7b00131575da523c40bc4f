#!/bin/sh
# test1.tcl \
exec tclsh "$0" ${1+"$@"}

#初始化（读取lib 读取全局变量）
source "C:\\Users\\ban\\workspace\\Test\\init.tcl"



#初始flag为1
set script_flag 1  

if { [catch { 
#预置条件
set TestCaseNo 333
#测试执行
set c "e:\\test\\2.txt"
set b [Capture 3 icmp $c 10 "c:\\program files\\wireshark"]

set d [Exsitcheck "192.168.20.10" $c]
puts $d

#结果判断
	
	
	
	
} ErrorMessage ]} {
  set script_flag 0    
  puts ErrorMessage:$ErrorMessage
}
	
	#Print script report

	return [SetTestcaseResult $TestCaseNo [expr {$script_flag == 1 ? "PASS" : "FAIL"}] ]
		
