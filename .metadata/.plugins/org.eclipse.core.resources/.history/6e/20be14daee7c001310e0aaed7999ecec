#!/bin/sh
# test1.tcl \
exec tclsh "$0" ${1+"$@"}

#初始化（读取lib 读取全局变量）
source "C:\\Users\\ban\\workspace\\Test\\init.tcl"

#读取CFG

	
       #初始flag为1
set script_flag 1  

if { [catch { 

RC_Client TestServer
	puts [TestServer cget mhost]
	

} ErrorMessage ]} {
  set script_flag 0    
  puts ErrorMessage:$ErrorMessage
}
	
	#Print script report

	SetTestcaseResult $TestCaseNo [expr {$script_flag == 1 ? "PASS" : "FAIL"}] 
	
	
	}
