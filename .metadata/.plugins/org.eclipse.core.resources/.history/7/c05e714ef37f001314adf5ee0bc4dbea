#!/bin/sh
# Dhcp.tcl \
exec tclsh "$0" ${1+"$@"}
#初始化（读取lib 读取全局变量）
proc StartExcute {ProjectLocation libary} {
set Location "$ProjectLocation\\$libary"
catch {glob -directory "$ProjectLocation\\$libary" *.app.tcl} msg
	
foreach file $msg {
source $file
}
}

StartExcute "C:\\Users\\ban\\workspace\\Test\\Scripts" "DHCP"